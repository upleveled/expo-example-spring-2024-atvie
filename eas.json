{
  "cli": {
    "version": ">= 10.0.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "extends": "base"
    },
    "production": {
      "extends": "base"
    },
    "base": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}